{{yield (hash
  selected-options=(component 'select-box/selected-options'
    class-prefix=attrs.class-prefix
    -register=(action 'registerSelectedOptionsContainer')
    -deregister=(action 'deregisterSelectedOptionsContainer'))
  selected-option=(component 'select-box/selected-option'
    class-prefix=attrs.class-prefix
    -active-index=activeSelectedOptionIndex
    -components=selectedOptions
    -activate=(action 'activateSelectedOptionAtIndex')
    -register=(action 'registerSelectedOption')
    -deregister=(action 'deregisterSelectedOption'))
  options=(component 'select-box/options'
    class-prefix=attrs.class-prefix
    -register=(action 'registerOptionsContainer')
    -deregister=(action 'deregisterOptionsContainer'))
  option=(component 'select-box/option'
    class-prefix=attrs.class-prefix
    -api=api
    -active-index=activeOptionIndex
    -components=options
    -activate=(action 'activateOptionAtIndex')
    -multiple=isMultiple
    -selected-value=(readonly selectedValue)
    -register=(action 'registerOption')
    -deregister=(action 'deregisterOption')
    -select=(action 'select'))
  group=(component 'select-box/group'
    class-prefix=attrs.class-prefix)
  input=(component 'select-box/input'
    class-prefix=attrs.class-prefix
    disabled=isDisabled
    -api=api
    -on-input=(action 'inputText')
    -register=(action 'registerInput')
    -deregister=(action 'deregisterInput'))
  isSearching=isSearching
  isSlowSearch=isSlowSearch
  value=selectedValue
  open=(action 'open')
  close=(action 'close')
  toggle=(action 'toggle')
  select=(action 'select')
  update=(action 'update')
  selectActiveOption=(action 'selectActiveOption')
  search=(action 'search')
  setInputValue=(action 'setInputValue')
  focusInput=(action 'focusInput')
  activateOptionAtIndex=(action 'activateOptionAtIndex')
  activateNextOption=(action 'activateNextOption')
  activatePreviousOption=(action 'activatePreviousOption')
  deactivateOptions=(action 'deactivateOptions')
  activateSelectedOptionAtIndex=(action 'activateSelectedOptionAtIndex')
  activateNextSelectedOption=(action 'activateNextSelectedOption')
  activatePreviousSelectedOption=(action 'activatePreviousSelectedOption')
  deactiveSelectedOptions=(action 'deactivateSelectedOptions')
)}}
